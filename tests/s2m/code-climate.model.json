{
  "": "$ANY",
  "#": "Configuration file as an alternative for configuring your repository in the settings page.",
  "$": {
    "config": {
      "": "$ANY"
    },
    "enabled": {
      "": "$ANY",
      "?enabled": {
        "@": true
      }
    },
    "threshold": {
      "|": [
        -1,
        null
      ]
    }
  },
  "?checks": {
    "": "$ANY",
    "?argument-count": {
      "@": "$enabled"
    },
    "?complex-logic": {
      "@": "$enabled"
    },
    "?file-lines": {
      "@": "$enabled"
    },
    "?identical-code": {
      "@": "$enabled"
    },
    "?method-complexity": {
      "@": "$enabled"
    },
    "?method-count": {
      "@": "$enabled"
    },
    "?method-lines": {
      "@": "$enabled"
    },
    "?nested-control-flow": {
      "@": "$enabled"
    },
    "?return-statements": {
      "@": "$enabled"
    },
    "?similar-code": {
      "@": "$enabled"
    }
  },
  "?exclude_patterns": {
    "@": [
      {
        "@": ""
      }
    ]
  },
  "?plugins": {
    "": "$enabled",
    "#": "To add a plugin to your analysis. You can find the complete list of available plugins here: https://docs.codeclimate.com/docs/list-of-engines"
  },
  "?prepare": {
    "@": [
      {
        "": "$ANY",
        "?path": {
          "@": ""
        },
        "?url": {
          "@": "$URI"
        }
      }
    ]
  },
  "?version": {
    "#": "Required to adjust maintainability checks.",
    "@": ""
  }
}
