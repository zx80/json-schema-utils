            "_netcoreapp2.2",
            "_netcoreapp3.0",
            "_netcoreapp3.1",
            "_net11",
            "_net20",
            "_net35",
            "_net40",
            "_net403",
            "_net45",
            "_net451",
            "_net452",
            "_net46",
            "_net461",
            "_net462",
            "_net47",
            "_net471",
            "_net472",
            "_net48"
