    "?SpaceWithinExpressionParentheses": {
      "@": true
    },
    "?SpaceWithinMethodCallParentheses": {
      "@": true
    },
    "?SpaceWithinMethodDeclarationParenthesis": {
      "@": true
    },
    "?SpaceWithinOtherParentheses": {
      "@": true
    },
    "?SpaceWithinSquareBrackets": {
      "@": true
    },
    "?SpacesIgnoreAroundVariableDeclaration": {
      "@": true
    },
    "?SpacingAfterMethodDeclarationName": {
      "@": true
    },
    "?SpacingAroundBinaryOperator": {
      "@": ""
    },
    "?TabSize": {
      "@": -1
    },
    "?UseTabs": {
      "@": true
    },
    "?WrappingKeepStatementsOnSingleLine": {
      "@": true
    },
    "?WrappingPreserveSingleLine": {
      "@": true
    }
  },
  "?ImplementTypeOptions": {
    "": "$ANY",
    "?InsertionBehavior": {
      "|": [
        "_WithOtherMembersOfTheSameKind",
        "_AtTheEnd"
      ]
    },
    "?PropertyGenerationBehavior": {
      "|": [
        "_PreferThrowingProperties",
        "_PreferAutoProperties"
      ]
    }
  },
  "?Plugins": {
    "": "$ANY",
    "?LocationPaths": {
      "@": [
        ""
      ]
    }
  },
  "?RenameOptions": {
    "": "$ANY",
    "?RenameInComments": {
      "@": true
    },
    "?RenameInStrings": {
      "@": true
    },
    "?RenameOverloads": {
      "@": true
    }
  },
  "?RoslynExtensionsOptions": {
    "": "$ANY",
    "#": "Used to define refactorings, code actions and analyzer locations that OmniSharp should use (i.e. Roslynator)",
    "?DocumentAnalysisTimeoutMs": {
      "@": -1
    },
    "?EnableAnalyzersSupport": {
      "#": "If set to false, only refactorings are available",
      "@": true
    },
    "?EnableDecompilationSupport": {
      "@": true
    },
    "?LocationPaths": {
      "@": [
        ""
      ]
    }
