      }
    }
  },
  "|": [
    {
      "": "$ANY",
      "?additionalProperties": "$NONE",
      "?allow_duplicates": {
        "@": true
      },
      "?collections": "$collections",
      "?dependencies": {
        "@": [
          "$DependencyModel"
        ]
      },
      "?galaxy_info": "$GalaxyInfoModel"
    },
    null
  ]
}
